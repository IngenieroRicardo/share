<?xml version="1.0" encoding="UTF-8"?>
<schemalist>
	<schema path="/org/gnome/anjuta/plugins/python/" id="org.gnome.anjuta.plugins.python">
		<key type="b" name="no-rope-warning">
			<default>false</default>
		</key>
		<key type="b" name="completion-enable">
			<default>true</default>
		</key>
		<key type="b" name="completion-space-after-func">
			<default>true</default>
		</key>
		<key type="b" name="completion-brace-after-func">
			<default>true</default>
		</key>
		<key type="b" name="completion-closebrace-after-func">
			<default>true</default>
		</key>
		<key type="b" name="calltip-enable">
			<default>true</default>
		</key>
		<key type="s" name="interpreter-path">
			<default>"/usr/bin/python"</default>
		</key>
	</schema>
</schemalist>